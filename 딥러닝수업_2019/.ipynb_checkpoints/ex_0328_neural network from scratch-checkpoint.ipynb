    "\n",
    "## 시그모이드 함수\n",
    "\n",
    "import numpy as np\n",
    "import matplotlib.pylab as plt\n",
    "\n",
    "def sigmoid(x):\n",
    "    return 1/ (1+np.exp(-x))\n",
    "print(sigmoid(1))\n",
    "\n",
    "\n",
    "print(sigmoid(np.array([0,0.5,1])))\n",
    "\n",
    "x= np.arange(-10.0, 10.0, 0.1)\n",
    "y=sigmoid(x)\n",
    "\n",
    "plt.plot(x,y)\n",
    "plt.ylim(-0.1,1.1)\n",
    "plt.show()\n",
    "\n"
   ]
